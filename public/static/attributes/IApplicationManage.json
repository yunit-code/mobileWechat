{
    "classId": "idm.componet.mobilewechat.applicationmanage",
    "comName": "应用管理",
    "className": "IApplicationManage",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "标题容器样式",
            "desc": "点击？查看标题容器样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "titleBox"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "背景色",
                    "bindKey": "titleBgColor",
                    "placeholder": "请选择颜色",
                    "default": {}
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字",
                    "bindKey": "titleFontManage"
                }
            ]
        },
        {
            "type": "group",
            "text": "内层容器样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "图片宽度",
                    "desc": "填写应用图片的宽度，单位px",
                    "bindKey": "applicationImgWidthManage",
                    "default": 40
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字",
                    "bindKey": "fontManage"
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "boxManage"
                },
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "widthManage",
                            "width": "60px",
                            "default": "auto"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "heightManage",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "背景设置",
                    "desc": "点击？查看背景设置用法指南",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColorManage",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "uploadImage",
                            "layoutType": "inline",
                            "text": "背景图片",
                            "bindKey": "bgImgUrlManage",
                            "placeholder": "可输入图片地址或直接上传"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "横向偏移",
                            "bindKey": "positionXManage",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrlManage]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
                            "bindKey": "positionYManage",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrlManage]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "背景大小",
                            "bindKey": "bgSizeManage",
                            "dictionary": [
                                {
                                    "label": "裁切显示",
                                    "value": "cover"
                                },
                                {
                                    "label": "完全显示",
                                    "value": "contain"
                                },
                                {
                                    "label": "自定义",
                                    "value": "custom"
                                }
                            ],
                            "display": "@[bgImgUrlManage]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "宽度",
                            "bindKey": "bgSizeWidthManage",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeManage=='custom']"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "高度",
                            "bindKey": "bgSizeHeightManage",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeManage=='custom']"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "平铺模式",
                            "bindKey": "bgRepeatManage",
                            "dictionary": [
                                {
                                    "label": "双向重复",
                                    "value": "repeat"
                                },
                                {
                                    "label": "水平重复",
                                    "value": "repeat-x"
                                },
                                {
                                    "label": "垂直重复",
                                    "value": "repeat-y"
                                },
                                {
                                    "label": "不重复",
                                    "value": "no-repeat"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlManage]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "inline",
                            "text": "背景模式",
                            "bindKey": "bgAttachmentManage",
                            "dictionary": [
                                {
                                    "label": "固定",
                                    "value": "fixed"
                                },
                                {
                                    "label": "滚动",
                                    "value": "scroll"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlManage]"
                        }
                    ]
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "borderManage"
                }
            ]
        },
        {
            "type": "table",
            "text": "主题设置",
            "desc": "点击？查看主题设置用法指南",
            "bindKey": "themeList",
            "tabText": "主题[index]",
            "children": [
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "主题标识",
                    "bindKey": "key",
                    "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里选择blue",
                    "default":"blue",
                    "placeholder":"例如：blue",
                    "ctrlAttrObject": {
                        "mode": "combobox"
                    },
                    "dictionary": [
                        {
                            "label": "blue",
                            "value": "blue"
                        },
                        {
                            "label": "red",
                            "value": "red"
                        },
                        {
                            "label": "green",
                            "value": "green"
                        }
                    ]
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "主要颜色",
                    "bindKey": "mainColor",
                    "desc":"主题的主要颜色",
                    "placeholder": "请选择颜色",
                    "display":"@[key]",
                    "default": {
                        "hex": "#1890FF",
                        "hex8": "#1890FFFF"
                    }
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "次要颜色",
                    "bindKey": "minorColor",
                    "desc":"主题的次要颜色",
                    "placeholder": "请选择颜色",
                    "display":"@[key]",
                    "default": {
                        "hex": "#e6f7ff",
                        "hex8": "#e6f7ffFF"
                    }
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看高级设置用法指南",
            "children": [
                {
                    "type": "group",
                    "text": "适配设置",
                    "children": [
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "基准值",
                            "desc": "此属性用于标记当前组件在显示效果最完美情况下的宽度大小（px），例如：此组件显示效果最佳宽度为414px，则此处设置为414即可，如果此组件最佳效果为当前配置的屏幕宽（px）的一半则只需要除以2即可。",
                            "bindKey": "adaptationBase",
                            "default": 414
                        },
                        {
                            "type": "inputNumber",
                            "layoutType": "inline",
                            "text": "适配比例",
                            "desc": "与基准值配合使用，如果在实际预览屏幕宽为基准值的两倍，则此组件所有可适配的内容都会基于此处设置的适配比例相对应的进行放大缩小，例如此处设置1.2，则会相对应的放大至1.2倍",
                            "bindKey": "adaptationPercent",
                            "default": "1.2"
                        }
                    ]
                }
            ]
        }
    ]
}