{
    "classId": "idm.componet.mobilewechat.applicationsearch",
    "comName": "应用中心搜索",
    "className": "IApplicationSearch",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "group",
                    "text": "组件默认样式",
                    "children": [
                        {
                            "type": "box",
                            "layoutType": "block",
                            "text": "内外边距",
                            "bindKey": "box"
                        },
                        {
                            "type": "inlineGroup",
                            "text": "宽高",
                            "children": [
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "宽",
                                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                                    "bindKey": "width",
                                    "width": "60px",
                                    "default": "auto"
                                },
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "高",
                                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                                    "bindKey": "height",
                                    "width": "60px",
                                    "default": "auto"
                                }
                            ]
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "border",
                            "layoutType": "block",
                            "text": "边框",
                            "bindKey": "border"
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "文字",
                            "bindKey": "font"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "操作按钮样式",
                    "children": [
                        {
                            "type": "inlineGroup",
                            "text": "宽高",
                            "children": [
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "宽",
                                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                                    "bindKey": "widthButton",
                                    "width": "60px",
                                    "default": "45px"
                                },
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "高",
                                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                                    "bindKey": "heightButton",
                                    "width": "60px",
                                    "default": "30px"
                                }
                            ]
                        },
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColorButton",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "文字",
                            "bindKey": "fontButton"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "操作按钮样式(已添加)",
                    "children": [
                        {
                            "type": "inlineGroup",
                            "text": "宽高",
                            "children": [
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "宽",
                                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                                    "bindKey": "widthButtonDisabled",
                                    "width": "60px",
                                    "default": "45px"
                                },
                                {
                                    "type": "input",
                                    "layoutType": "block",
                                    "text": "高",
                                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                                    "bindKey": "heightButtonDisabled",
                                    "width": "60px",
                                    "default": "30px"
                                }
                            ]
                        },
                        {
                            "type": "group",
                            "text": "背景设置",
                            "desc": "点击？查看背景设置用法指南",
                            "helpUrl": "",
                            "children": [
                                {
                                    "type": "colorPicker",
                                    "layoutType": "inline",
                                    "text": "背景色",
                                    "bindKey": "bgColorButtonDisabled",
                                    "placeholder": "请选择颜色",
                                    "default": {}
                                }
                            ]
                        },
                        {
                            "type": "font",
                            "layoutType": "block",
                            "text": "文字",
                            "bindKey": "fontButtonDisabled"
                        }
                    ]
                }
            ]
        },
        {
            "type": "table",
            "text": "主题设置",
            "desc": "点击？查看主题设置用法指南",
            "bindKey": "themeList",
            "tabText": "主题[index]",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "主题标识",
                    "bindKey": "key",
                    "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                    "default":"",
                    "placeholder":"例如：blue"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "主要颜色",
                    "bindKey": "mainColor",
                    "desc":"主题的主要颜色",
                    "placeholder": "请选择颜色",
                    "display":"@[key]",
                    "default": {
                        "hex": "#1890FF",
                        "hex8": "#1890FFFF"
                    }
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "次要颜色",
                    "bindKey": "minorColor",
                    "desc":"主题的次要颜色",
                    "placeholder": "请选择颜色",
                    "display":"@[key]",
                    "default": {
                        "hex": "#e6f7ff",
                        "hex8": "#e6f7ffFF"
                    }
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看高级设置用法指南",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "全部应用",
                    "desc": "用于获取全部应用数据的接口地址",
                    "bindKey": "getAllApplicationUrl",
                    "default": "/ctrl/tencentApp/queryAppByGrant"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "添加应用",
                    "desc": "用于添加应用的接口地址",
                    "bindKey": "addApplicationUrl",
                    "default": "/ctrl/tencentApp/setFavoriteApp"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "点击[取消]时动作",
                    "bindKey": "clickCancelFunction",
                    "desc": "点击取消的时候会调用设置的自定义函数，接收参数为格式为{...自定义的}",
                    "multiple": true
                }
            ]
        }
    ]
}